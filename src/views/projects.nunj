{% extends 'layout.nunj' %}

{% block content %}

    {% for project in data.projects %}
        {{ utils.getProjectContent(project.descriptionFile) | safe }}
        <em>Technologies</em>
        <ul class="nav nav-pills">
        {% for skill in project.skills %}
            <li class="active"><a href="javascript:void(0)">{{ utils.getSkillById(skill).name or skill }}</a></li>
        {% endfor %}
        </ul>
        <hr/>
    {% endfor %}
{% endblock %}